	<queryString language="SQL">
		<![CDATA[SELECT
     Cuenta.`nombre` AS Cuenta_nombre,
     Cuenta.`tipo_saldo` AS Cuenta_tipo_saldo,
     Detalle_Libro_Diario.`cargo` AS Detalle_Libro_Diario_cargo,
     Detalle_Libro_Diario.`abono` AS Detalle_Libro_Diario_abono,
     Libro_Diario.`fecha` AS Libro_Diario_fecha,
     Cuenta.`saldo` AS Cuenta_saldo
FROM
     `Cuenta` Cuenta INNER JOIN `Detalle_Libro_Diario` Detalle_Libro_Diario ON Cuenta.`codigo` = Detalle_Libro_Diario.`codigo`
